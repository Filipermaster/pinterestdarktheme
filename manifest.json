{
    "manifest_version": 3,
    "name": "Pinterest DARK THEME",
    "version": "2.0",
    "description": "Apply a different themes to Pinterest web version.",
    "author": "Iris",
    "icons": {
        "128": "icon.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*.pinterest.com/*",
                "https://*.pinterest.fr/*",
                "https://*.pinterest.de/*",
                "https://*.pinterest.co.uk/*",
                "https://*.pinterest.jp/*",
                "https://*.pinterest.ru/*",
                "https://*.pinterest.cn/*",
                "https://*.pinterest.pt/*",
                "https://*/pinterest.it/*",
                "https://*/pinterest.es/*",
                "https://*/pinterest.ca/*",
                "https://*/pinterest.com.au/*"
            ],
            "css": [
                "main.css"
            ],
            "js": [
                "main.js"
            ]
        }
    ]
}